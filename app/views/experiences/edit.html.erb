<%= form_tag "/experiences/#{@experience['id']}", method: :patch do %>

  <%= label_tag :company_name %><br>
  <%= text_field_tag :company_name, @experience['company_name'] %><br>
  <%= label_tag :job_title %><br>
  <%= text_field_tag :job_title, @experience['job_title'] %><br>
  <%= label_tag :start_date %><br>
  <%= text_field_tag :start_date, @experience['start_date'] %><br>
  <%= label_tag :end_date %><br>
  <%= text_field_tag :end_date, @experience['end_date'] %><br>
  <%= label_tag :details %><br>
  <%= text_field_tag :details, @experience['details'] %><br>

<%= submit_tag "Submit Update" %>

<% end %>
