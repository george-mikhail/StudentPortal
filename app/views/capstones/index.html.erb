<h1>Capstones</h1>
<a href="/experiences/new">Create New Experience</a>
<% @capstones.each do |capstone| %>
<div>
  <p> Capstone Name: <%= capstone['name'] %> </p>
  <p> Short Description: <%= capstone['description'] %> </p>
  <p> Website: <%= capstone['url'] %> </p>
  <img scr="<%= capstone['screenshot']%>">
  <%= link_to "Edit Capstone Project", "/capstones/#{capstone['id']}", class: "btn btn-success" %>
</div>

<% end %>





